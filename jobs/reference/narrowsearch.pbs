#!/bin/bash -l

#PBS -N narrowsearch
#PBS -l walltime=20:00:00
#PBS -l mem=1gb
#PBS -l ncpus=1
#PBS -J 1-40

source activate tf-cpu

cd $PBS_O_WORKDIR

let INDEX=${PBS_ARRAY_INDEX}+1
IFS=',' read params <<< `sed -n "$INDEX p" input_narrow.csv`
paramArray=($params)

i=${paramArray[0]}
j=${paramArray[1]}
k=${paramArray[2]}

cd ..
python3 conv_ca.py --num_layers=$i --state_size=$j --run=$k --max_steps=30000 --logfile="narrow3"
